#include<stdio.h>
int main()
{
	int a,b;
	printf("ENTER THE VALUES");
	scanf("%d%d",&a,&b);
	if(a>b)
	printf("a is greater than b");
	else
	printf("a is less than b");
	return 0;
}




OUTPUT
ENTER THE VALUES 80 60
a is greater than b
