#include <stdio.h>
int main() 
{
int num[10];
for (int i = 0; i < 10; i++) {
printf("Enter number %d: ", i + 1);
scanf("%d", &num[i]);
}
printf("You entered: ");
for (int i = 0; i < 10; i++) {
printf("%d ", num[i]);
}
return 0;
}



OUTPUT
Enter number 1: 56
Enter number 2: 30
Enter number 3: 2
Enter number 4: 4
Enter number 5: 5
Enter number 6: 24
Enter number 7: 67
Enter number 8: 87
Enter number 9: 47
Enter number 10: 22
